import java.util.*;

public class Main
{
    static int coin (int n){
        int a = 0;
        while(n!=0){
            a=a+(n*n);
            n--;
        }
        return a;
    }

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int res = coin(n);
		System.out.println(res);
	}
}
