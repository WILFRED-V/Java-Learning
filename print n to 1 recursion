import java.util.*;
public class Main{
    static void name(int i,int n){
        if(i<1){
            return;
        }
        System.out.print(i + " ");
        name(i-1,n);
    }
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        name(4,n);
    }
}
