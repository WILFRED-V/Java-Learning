import java.util.*;

public class Main {
    static int run(int[] arr) {
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] != i + 1) {
                return i + 1;
            }
        }

        return arr.length + 1;
    }

    public static void main(String[] args) {
        int arr[] = {1, 2, 3, 4, 5, 6};
        int res = run(arr);
        System.out.println("Missing number: " + res); 
    }
}
