public class Main{
    public static void main(String[]args){
        String a = "malayalam";
        String b = a;
        String c = "";
        for(int i=b.length()-1;i>=0;i--){
            c+=a.charAt(i);
        }
        System.out.println(a);
        System.out.println(c);
        
        if(b.equals(c)){
            System.out.print("palindrome");
        }
        else{
        System.out.println("not palind");
        }
    }
}
