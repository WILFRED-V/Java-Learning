public class Main
{
	public static void main(String[] args) {

        String s = "Programming";
        int hash[] = new int[256];
        for (int i = 0; i < s.length(); i++) {
            char ch = s.charAt(i);
            hash[ch]++;
        }
        for(int i=0;i<hash.length;i++){
            if(hash[i]>1){
                System.out.println((char)i +" "+ hash[i]);
            }
        }
	}
}
